{
  "name": "@pulseboard/react-native",
  "version": "0.1.0",
  "description": "Official PulseBoard SDK for React Native",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.73"
  },
  "dependencies": {
    "react-native-device-info": "^10.0.0",
    "@react-native-community/netinfo": "^11.0.0"
  },
  "devDependencies": {
    "@types/react": "*",
    "@types/react-native": "*",
    "typescript": "*",
    "tsup": "*"
  },
  "keywords": [
    "react-native",
    "monitoring",
    "observability",
    "error-tracking",
    "pulseboard"
  ],
  "license": "MIT",
  "author": {
    "email": "m.arslanmushtaqahmed@gmail.com",
    "name": "Arslan Mushtaq",
    "url": "https://arslanmushtaq.com"
  }
}
